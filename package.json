  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
